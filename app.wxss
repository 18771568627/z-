/**app.wxss**/
.container {
  top:0;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
 
  box-sizing: border-box;
} 
/* 页脚 */
.footer{
  margin-top:15rpx;
  width:100%;height: 180rpx; 
  text-align: center;
  vertical-align: middle;
  background-color:#eee;
}
.footer>image{
  width:40%;height:80rpx;
  margin-top: 50rpx;
}


/* 箭头 */
.col{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.col::after{
  content:"";
  width: 15rpx;height: 15rpx;
  border-top:3px solid #ccc;
  border-right: 3px solid #ccc;
  transform: rotate(45deg);
  margin-right:20rpx;
}
